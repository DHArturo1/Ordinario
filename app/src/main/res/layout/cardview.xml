<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="15dp"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="@color/colorINEC">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <de.hdodenhof.circleimageview.CircleImageView xmlns:app="https://schemas.android.com/apk/res-auto"
                android:id="@+id/profile_image"
                android:layout_width="96dp"
                android:layout_height="96dp"
                android:src="@drawable/imagea"
                app:civ_border_color="#FF000000"
                app:civ_border_width="2dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/ConsultaNombre"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaNombre" />

                <TextView
                    android:id="@+id/ConsultaDomicilio"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaDomicilio" />

                <TextView
                    android:id="@+id/ConsultaFechaNacimiento"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaFechaNacimiento" />

                <TextView
                    android:id="@+id/ConsultaGenero"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaGenero" />

                <TextView
                    android:id="@+id/ConsultaClaveElectoral"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaClaveElectoral" />

                <TextView
                    android:id="@+id/ConsultaCurp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaCurp" />

                <TextView
                    android:id="@+id/ConsultaAnioRegistro"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaAnioRegistro" />

                <TextView
                    android:id="@+id/ConsultaNumeroVersion"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaNumeroVersion" />

                <TextView
                    android:id="@+id/ConsultaDistrito"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaDistrito" />

                <TextView
                    android:id="@+id/ConsultaSeccion"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaSeccion" />

                <TextView
                    android:id="@+id/ConsultaLocalidad"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaLocalidad" />

                <TextView
                    android:id="@+id/ConsultaAnioEmision"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaAnioEmision" />

                <TextView
                    android:id="@+id/ConsultaVigencia"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/ConsultaVigencia" />

            </LinearLayout>

        </LinearLayout>
    </androidx.cardview.widget.CardView>


    <!--android:layout_marginStart="8dp"
    android:layout_marginTop="8dp"
    android:layout_marginEnd="8dp"-->
    <!--app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"-->
</LinearLayout>